<p id="notice"><%= notice %></p>
<h1><%= t "skill.listing_skill" %></h1>
<table>
  <thead class="t-header">
    <tr>
      <th class="th1"><%= t "skill.name" %></th>
      <th class="th2"><%= t "skill.description" %></th>
      <th colspan="3" class="th-colspan"></th>
    </tr>
  </thead>

  <tbody class="t-body">
    <%= render partial: "skill", collection: @skills %>
    <%= will_paginate %>
  </tbody>
</table>
<h2><%= t "import.skill" %></h2>
<%= form_tag admin_skill_imports_path, method: :post, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<%= link_to t("skill.new_skill"), new_admin_skill_path %>
