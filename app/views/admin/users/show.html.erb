<h1><%= t "user.profile" %></h1>
  <div>
    <h3><%= t "user.email" %></h3>
    <h2><%= @user.email %></h2>
  </div>
  <div>
    <h3><%= t "user.positions" %></h3>
      <% @positions.each do |n| %>
        <%= n.name %>
      <% end %>
    </h2>
  </div>
  <div>
    <h3><%= t "user.skill" %></h3>
    <h2>
      <% @skills.each do |n| %>
        <%= n.name %> |
      <% end %>
    </h2>
  </div>
  <%= link_to t("user.add_position"), admin_user_position_users_path(@user), class: "btn btn-primary" %>
  <div class="box-content">
    <ul class="account_list">
      <li><%= link_to t("user.list-team"), admin_teams_path %></li>
    </ul>
  </div>
