<h1><%= t "user.all_user" %></h1>
<p>
  <%= t "user.download_skill" %>
  <%= link_to t("user.csv"), admin_skills_path(format: "csv") %>
</p>
<p>
  <%= t "user.download_team" %>
  <%= link_to t("user.csv"), admin_teams_path(format: "csv") %>
</p>
<p>
  <%= t "user.download_project" %>
  <%= link_to t("user.csv"), admin_projects_path(format: "csv") %>
</p>
<p>
  <%= t "user.download_position" %>
  <%= link_to t("user.csv"), admin_positions_path(format: "csv") %>
</p>
<%= search_form_for @search, url: admin_users_path do |f| %>
  <fieldset>
    <ul>
      <li>
        <%= f.label :email_cont %>
        <%= f.text_field :email_cont %>
      </li>
    </ul>
  </fieldset>
  <%= f.submit %>
<% end %>

<%= render "result" %>

<div id = "activity-log">
  <h3><%= t "activity.list" %></h3>
  <%= render "activity" %>
<%= flash[:notice] %>

<div>
<h3><%= t "import.import_file" %></h3>
  <%= form_tag import_admin_users_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>
