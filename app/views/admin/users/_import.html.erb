<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users_csv.each do |u| %>
      <tr>
        <td><%= u.id %></td>
        <td><%= u.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a CSV File</h3>
  <%= form_tag import_admin_users_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>

<%= flash[:notice] %>
<div>
  <h3><%= t "import.import_file" %></h3>
  <%= form_tag import_admin_users_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag t"import.submit_tag" %>
  <% end %>
</div>
